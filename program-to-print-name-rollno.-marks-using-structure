#include <stdio.h>
struct student 
{
    char name [10];
    int rno;
    int marks[5];
};
    int main()
{  int i;
    struct student s ;
    printf("name,rno");
    scanf("%s%d",s.name,&s.rno);   
    printf("%s      %d",s.name,s.rno);
    printf("\nyours marks");
    for (i=0;i<=3;i++)
        scanf("%d\n",&s.marks[i]);
    for (i=0;i<=3;i++)
        printf ("%d\n ",s.marks[i]);
    return 0;
}
