#include<stdio.h>
int main()
{   char ch;
    printf("enter the character ");
    scanf("%c",&ch);
    switch(ch)
{
    case 'A'...'Z':printf("Upper case Character");
                   break;
    case 'a'...'z':printf("Lower case character");
                   break;
    case'0'...'9':printf("Digit");
                   break;
    default:printf("special character");
}
    return 0;
}
