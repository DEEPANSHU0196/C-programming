#include <stdio.h>
void check ( char );
int main()
{
    char ch  ;
    //input the value ch 
    printf ("character");
    scanf("%c",& ch);
    check (ch);
    return 0;
}

 void check (char ch)
   {  if ((ch=='a')||(ch=='e')||(ch=='i')||(ch=='o')||(ch=='u'))
    printf ("vowel ");
    else 
    printf("consonant  ");
}
  
