#include<stdio.h>
int main()
{
    int A[2][2],B[2][2],C[2][2],i,j;
        printf("Input for 1st 2D array ");
        
    for (i=0;i<2;i++)
    {
        for (j=0;j<2;j++)
        scanf("%d",&A[i][j]);
    }
        printf("Input for 2nd 2D array");
    for (i=0;i<2;i++)
    {
        for (j=0;j<2;j++)
            scanf("%d",&B[i][j]);
    }
    for (i=0;i<2;i++)
    {
        for (j=0;j<2;j++)
            C[i][j]=A[i][j]+B[i][j];
    }
    for (i=0;i<2;i++)
    {
        for (j=0;j<2;j++)
            printf("Sum=%d\n",C[i][j]);
    }
}
 
